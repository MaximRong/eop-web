<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <description>monitor do not delete</description>
        <servlet-name>HealthMonitor</servlet-name>
        <servlet-class>org.phw.eop.HealthMonitor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HealthMonitor</servlet-name>
        <url-pattern>/HealthMonitor/welcome</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>EOP Servlet</servlet-name>
        <servlet-class>org.phw.eop.serv.EopServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>EOP Servlet</servlet-name>
        <url-pattern>*</url-pattern>
    </servlet-mapping>
</web-app>
